int zAlgorithm(string s, string p, int n, int m)

{

    int count=0;

    int index=0;

    while(1){

        index=s.find(p);

                if (index >= 0 && index <= s.length() - 1) {

                  count++;

                  s[index] = '1';

                }

                else{

                    break;

                }

        }

    return count;

}

